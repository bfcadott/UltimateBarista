<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="edu.oakland.ultimatebarista.Game"
    android:background="@android:color/holo_orange_light">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton32"
        android:src="@android:drawable/ic_dialog_dialer"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton14"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#ff000000"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton15"
        android:background="#ff000000"
        android:layout_below="@+id/imageButton14"
        android:layout_alignLeft="@+id/imageButton14"
        android:layout_alignStart="@+id/imageButton14"
        android:layout_marginTop="10dp" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton16"
        android:background="#ff000000"
        android:layout_below="@+id/imageButton15"
        android:layout_alignLeft="@+id/imageButton15"
        android:layout_alignStart="@+id/imageButton15"
        android:layout_marginTop="10dp" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton17"
        android:background="#ff000000"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/imageButton14"
        android:layout_toEndOf="@+id/imageButton14"
        android:layout_marginLeft="50dp" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton18"
        android:background="#ff000000"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/imageButton17"
        android:layout_toEndOf="@+id/imageButton17"
        android:layout_marginLeft="15dp" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton19"
        android:background="#ff000000"
        android:layout_above="@+id/imageButton16"
        android:layout_alignLeft="@+id/imageButton18"
        android:layout_alignStart="@+id/imageButton18" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton20"
        android:background="#ff000000"
        android:layout_alignTop="@+id/imageButton15"
        android:layout_toLeftOf="@+id/imageButton18"
        android:layout_toStartOf="@+id/imageButton18" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton21"
        android:background="#ff000000"
        android:layout_above="@+id/imageButton18"
        android:layout_toRightOf="@+id/imageButton23"
        android:layout_toEndOf="@+id/imageButton23"
        android:layout_marginLeft="15dp" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton23"
        android:background="#ff000000"
        android:layout_above="@+id/imageButton18"
        android:layout_toRightOf="@+id/imageButton18"
        android:layout_toEndOf="@+id/imageButton18"
        android:layout_marginLeft="50dp" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton22"
        android:background="#ff000000"
        android:layout_alignTop="@+id/imageButton21"
        android:layout_toRightOf="@+id/imageButton21"
        android:layout_toEndOf="@+id/imageButton21"
        android:layout_marginLeft="15dp" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton24"
        android:background="#ff000000"
        android:layout_below="@+id/imageButton18"
        android:layout_alignLeft="@+id/imageButton23"
        android:layout_alignStart="@+id/imageButton23" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton25"
        android:background="#ff000000"
        android:layout_alignTop="@+id/imageButton24"
        android:layout_alignLeft="@+id/imageButton21"
        android:layout_alignStart="@+id/imageButton21" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton26"
        android:background="#ff000000"
        android:layout_alignTop="@+id/imageButton25"
        android:layout_toRightOf="@+id/imageButton25"
        android:layout_toEndOf="@+id/imageButton25"
        android:layout_marginLeft="15dp" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton27"
        android:background="#ff000000"
        android:layout_alignTop="@+id/imageButton16"
        android:layout_alignLeft="@+id/imageButton24"
        android:layout_alignStart="@+id/imageButton24" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton28"
        android:background="#ff000000"
        android:layout_alignTop="@+id/imageButton29"
        android:layout_alignLeft="@+id/imageButton25"
        android:layout_alignStart="@+id/imageButton25" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton29"
        android:background="#ff000000"
        android:layout_alignTop="@+id/imageButton27"
        android:layout_alignLeft="@+id/imageButton26"
        android:layout_alignStart="@+id/imageButton26" />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/imageButton30"
        android:background="#ff000000"
        android:layout_alignTop="@+id/imageButton18"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/imageButton31"
        android:background="#ff000000"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <RelativeLayout
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:background="#ff9000ff"
        android:padding="10dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/imageButton14"
        android:layout_alignStart="@+id/imageButton14"
        android:layout_above="@+id/imageButton14">

        <ListView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/listView"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>

    <Chronometer
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chronometer"
        android:textSize="20dp"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignRight="@+id/imageButton22"
        android:layout_alignEnd="@+id/imageButton22" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Morning Rush"
        android:id="@+id/textView10"
        android:layout_centerHorizontal="true" />

    <ProgressBar
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/progressBar"
        android:layout_below="@+id/textView10"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:max="100"
        android:progress="50"
        android:indeterminate="false"
        android:layout_marginTop="5dp"
        android:layout_alignRight="@+id/chronometer"
        android:layout_alignEnd="@+id/chronometer" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="1/3 orders"
        android:id="@+id/textView11"
        android:layout_alignTop="@+id/imageButton32"
        android:textColor="#ffffffff"
        android:layout_marginTop="20dp"
        android:layout_alignRight="@+id/progressBar"
        android:layout_alignEnd="@+id/progressBar"
        android:layout_alignLeft="@+id/progressBar"
        android:layout_alignStart="@+id/progressBar"
        android:gravity="center" />

</RelativeLayout>
