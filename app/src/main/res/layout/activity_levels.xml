<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="edu.oakland.ultimatebarista.Levels"
    android:background="@drawable/schedule">

    <Button
        android:id="@+id/signOutButton"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Sign Out"
        android:visibility="invisible"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp" />

    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:layout_centerInParent="true"
        android:layout_alignParentRight="false">

        <ImageButton
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/level1button"
            android:layout_column="0"
            android:enabled = "false"
            android:layout_margin="0dp"
            android:layout_row="0"
            android:layout_marginRight="10dp"
            android:src="@drawable/coffeecupltgray"
            android:scaleType="fitXY" />

        <ImageButton
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/level2button"
            android:layout_column="1"
            android:enabled = "false"
            android:layout_margin="0dp"
            android:layout_row="0"
            android:layout_marginRight="10dp"
            android:src="@drawable/coffeecupltgray"
            android:scaleType="fitXY" />

        <ImageButton
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/level3button"
            android:layout_column="2"
            android:enabled = "false"
            android:layout_margin="0dp"
            android:layout_row="0"
            android:layout_marginRight="10dp"
            android:src="@drawable/coffeecupltgray"
            android:scaleType="fitXY" />

        <ImageButton
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/level4button"
            android:layout_column="3"
            android:enabled = "false"
            android:layout_margin="0dp"
            android:layout_row="0"
            android:layout_marginRight="10dp"
            android:src="@drawable/coffeecupltgray"
            android:scaleType="fitXY" />

        <ImageButton
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/level5button"
            android:layout_column="4"
            android:enabled = "false"
            android:layout_margin="0dp"
            android:layout_row="0"
            android:layout_marginRight="10dp"
            android:src="@drawable/coffeecupltgray"
            android:scaleType="fitXY" />

        <ImageButton
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/level6button"
            android:layout_column="5"
            android:enabled = "false"
            android:layout_margin="0dp"
            android:layout_row="0"
            android:src="@drawable/coffeecupltgray"
            android:scaleType="fitXY" />

        <ImageButton
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/level7button"
            android:layout_column="0"
            android:enabled = "false"
            android:layout_margin="0dp"
            android:layout_row="1"
            android:layout_marginRight="10dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/coffeecupltgray"
            android:scaleType="fitXY" />

        <ImageButton
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/level8button"
            android:layout_column="1"
            android:enabled = "false"
            android:layout_margin="0dp"
            android:layout_row="1"
            android:layout_marginRight="10dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/coffeecupltgray"
            android:scaleType="fitXY" />

        <ImageButton
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/level9button"
            android:layout_column="2"
            android:enabled = "false"
            android:layout_margin="0dp"
            android:layout_row="1"
            android:layout_marginRight="10dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/coffeecupltgray"
            android:scaleType="fitXY" />

        <ImageButton
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/level10button"
            android:layout_column="3"
            android:enabled = "false"
            android:layout_margin="0dp"
            android:layout_row="1"
            android:layout_marginRight="10dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/coffeecupltgray"
            android:scaleType="fitXY" />

        <ImageButton
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/level11button"
            android:layout_column="4"
            android:layout_margin="0dp"
            android:layout_row="1"
            android:layout_marginRight="10dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/coffeecupltgray"
            android:scaleType="fitXY" />

        <ImageButton
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/level12button"
            android:layout_column="5"
            android:enabled = "false"
            android:layout_margin="0dp"
            android:layout_row="1"
            android:layout_marginRight="10dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/coffeecupltgray"
            android:scaleType="fitXY" />
    </GridLayout>

</RelativeLayout>
